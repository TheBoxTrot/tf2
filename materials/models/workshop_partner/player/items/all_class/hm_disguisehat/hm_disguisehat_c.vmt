"VertexlitGeneric"
{
	"$basetexture" "models/workshop_partner/player/items/all_class/hm_disguisehat/hm_disguisehat"
	"$bumpmap" "models/workshop_partner/player/items/all_class/hm_disguisehat/hm_disguisehat_normal"
	"$detail" "effects/tiledfire/fireLayeredSlowTiled512.vtf"	
	"$detailscale" "5"
	"$detailblendfactor" .0025
	"$detailblendmode" 6
	"$yellow" "1"
	"$phong" "1"
	"$phongexponent" "5"
	"$phongboost" "3"	
	"$lightwarptexture" "models/player/pyro/pyro_lightwarp"
	"$phongfresnelranges"	"[.3 1 8]"
	"$normalmapalphaphongmask" "1"
	
 	"$blendtintbybasealpha" "1" 
 	"$blendtintcoloroverbase" "0"
 
 	"$color2" "{ 63 45 43}"
	"$colortint_base" "{ 63 45 43}"
	"$colortint_tmp" "[0 0 0]" 
	
	// Rim lighting parameters
	"$rimlight" "1"												// To enable rim lighting (requires phong)
	"$rimlightexponent" "4"										// Exponent for phong component of rim lighting	
	"$rimlightboost" "2"										// Boost for ambient cube component of rim lighting
	
	"$glowcolor" "1"
	// Cloaking
	"$cloakPassEnabled" "1"
	"Proxies"
	{
		"invis"
		{
		}
		"AnimatedTexture"
		{
			"animatedtexturevar" "$detail"
			"animatedtextureframenumvar" "$detailframe"
			"animatedtextureframerate" 30
		}
		"BurnLevel"
		{
			"resultVar" "$detailblendfactor"
		}
		"ItemTintColor"
		{
			"resultVar" "$colortint_tmp"
		}
		"SelectFirstIfNonZero"
		{
			"srcVar1"	"$colortint_tmp"
			"srcVar2"	"$colortint_base"
			"resultVar" "$color2"
		}
		"YellowLevel"
		{
			"resultVar" "$yellow"
		}
		"Multiply"
		{
			"srcVar1" "$color2"
			"srcVar2" "$yellow"
			"resultVar" "$color2"
		}
	}
}