"VertexLitGeneric"
{
	"$basetexture" "models/workshop_partner/player/items/all_class/passtime_medal/passtime_medal_gold_blue"
	"$phong" "1"
	"$phongexponent" "25"
	"$phongboost" "5"	
	"$lightwarptexture" "models\buildables\sentry1\Sentry1_lightwarp"
	"$phongfresnelranges"	"[.1 .1 .1]"
	"$bumpmapalphaphongmask" "1"

	"$rimlight" "1"
	"$rimlightexponent" "4"	
	"$rimlightboost" ".25"

	"360?$color2" "[ 0.9 0.8 0.8 ]"

	">=DX90"
	{
		"$selfillum" "1"
	}

	"$glowcolor" "1"

	// Cloaking
	"$cloakPassEnabled" "1"

	"$yellow"		"0"
	"$color2"		"{ 255 255 255 }"

	"Proxies"
	{
		"invis"
		{
		}
		"AnimatedTexture"
		{
			"animatedtexturevar" "$detail"
			"animatedtextureframenumvar" "$detailframe"
			"animatedtextureframerate" 30
		}
		"BurnLevel"
		{
			"resultVar" "$detailblendfactor"
		}
		"YellowLevel"
		{
			"resultVar" "$yellow"
		}
		"Equals"
		{
			"srcVar1" "$yellow"
			"resultVar" "$color2"
		}
	}
}