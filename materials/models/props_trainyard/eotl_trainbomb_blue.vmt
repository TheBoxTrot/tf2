"VertexLitGeneric"
{
	"$basetexture" "models\props_trainyard/eotl_trainbomb_blue"
	"$bumpmap" "models/props_trainyard/eotl_trainbomb_normal"


	"$phong" "1"

	"$phongexponenttexture" "models\props_trainyard/eotl_trainbomb_rimmask"
