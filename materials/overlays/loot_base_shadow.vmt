"modulate"
{
	"$baseTexture" "overlays/loot_base_shadow"
	"$decal" 1
	"$mod2x" 1
	"$translucent" 1
	"$vertexcolor" "1"
	"$vertexalpha" "1"
	"%keywords" "tf"


	//Turning overlay off on low end machines

	// DX9 and up hardware
	">=DX90"
	{
		"$baseTexture" "overlays/loot_base_shadow"
	}

	// Pre DX9 hardware
	"<DX90"
	{
		"$baseTexture" 	"dev/null"
	}


}






